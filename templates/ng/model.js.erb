'use strict';

/*
 $update used for updating
 $save for saving
 $remove for removing
 query() to fetch all
 get to fetch specific
 */
app.factory("<%= model_name %>", function($resource) {
    return $resource("/<%= plural_table_name %>/:id", {id: "@id"}, {
    	update: {method: "PUT"},
    	query: {method:'GET',  isArray:true},
    	get: {method:'GET', params:{id:"@id"} }
    	});
});